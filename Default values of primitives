import java.util.*;
public class defaultvalues{
	boolean defaultBoolean;
	char defaultChar;
	byte defaultByte;
	short defaultShort;
	int defaultInt;
	long defaultLong;
	float defaultFloat;
	double defaultDouble;
	public static void main(String[] args){
		defaultvalues defaults =new defaultvalues();
		System.out.println("The default values of boolean: "+defaults.defaultBoolean);
		System.out.println("The default values of char:'null "+defaults.defaultChar+"'");
		System.out.println("The default values of byte: "+defaults.defaultByte);
		System.out.println("The default values of short: "+defaults.defaultShort);
		System.out.println("The default values of int: "+defaults.defaultInt);
		System.out.println("The default values of long: "+defaults.defaultLong);
		System.out.println("The default values of float: "+defaults.defaultFloat);
		System.out.println("The default values of double: "+defaults.defaultDouble);
	}
}
OUTPUT:
The default values of boolean: false
The default values of char:'null '
The default values of byte: 0
The default values of short: 0
The default values of int: 0
The default values of long: 0
The default values of float: 0.0
The default values of double: 0.0
